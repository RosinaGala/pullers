{
  "$schema": "vscode://schemas/color-theme",
  "name": "hein",
  "type": "dark",
  "colors": {
    // activityBar
    "activityBar.background": "#161217",
    // "activityBar.border": "#7EE5D4",
    "activityBar.dropBackground": "#000",
    "activityBar.foreground": "#fff",
    "activityBarBadge.background": "#7EE5D4",
    "activityBarBadge.foreground": "#000",
    // badge
    "badge.background": "#546461",
    "badge.foreground": "#000000",
    // button
    "button.background": "#AE91BB",
    "button.foreground": "#fff",
    "button.hoverBackground": "#5e3771",
    // contrast
    "contrastBorder": "#ffffff00",
    // debug
    "debugExceptionWidget.background": "#0b0b0b",
    "debugExceptionWidget.border": "#aaa",
    "debugToolBar.background": "#0b0b0b",
    // description
    "descriptionForeground": "#aaa",
    // diff
    "diffEditor.insertedTextBackground": "#C9BFFF11",
    "diffEditor.insertedTextBorder": "#C9BFFF55",
    "diffEditor.removedTextBackground": "#ee3a4311",
    "diffEditor.removedTextBorder": "#ee3a4355",
    // dropdown
    "dropdown.background": "#0b0b0b",
    "dropdown.border": "#1D181F",
    "dropdown.foreground": "#fff",
    // editor
    // This is the main background color
    "editor.background": "#161217",
    // this is the main text 
    "editor.foreground": "#fff",
    // Okay this part is confusing as heck!
    // Currently found item
    "editor.findMatchBackground": "#6f4d81",
    // Other Found Items int the document
    "editor.findMatchHighlightBackground": "#4e365b",
    // WTF is this one for? I don't know
    "editor.findRangeHighlightBackground": "#243E51",
    // When you hover over something and a popup shows, this highlights that thing
    "editor.hoverHighlightBackground": "#7EE5D433",
    // when you have something selected, but have lost focus on the editor
    "editor.inactiveSelectionBackground": "#0b0b0b",
    // current line styles
    "editor.lineHighlightBackground": "#2e3d3edb",
    "editor.rangeHighlightBackground": "#365b54",
    // selected Text colours
    // This is the standard Select colour
    "editor.selectionBackground": "#593436",
    // This is the colour of the other matching elements
    "editor.selectionHighlightBackground": "#402526",
    // if you tab away you can colour it differently, but ill leave this one out
    // "editor.inactiveSelectionBackground": "",
    // Word Highlights! This happens when you move your cursor inside a variable
    // Strong is the one where your cursor currently is
    "editor.wordHighlightStrongBackground": "#FFFFFF0D",
    // and this one is the rest of them
    "editor.wordHighlightBackground": "#593436",
    "editorBracketMatch.background": "#000",
    "editorBracketMatch.border": "#F0A3FF",
    "editorCodeLens.foreground": "#aaa",
    "editorCursor.foreground": "#7EE5D4",
    "editorError.border": "#000",
    "editorError.foreground": "#A22929",
    // gutter
    // "editorGutter.background": "",
    "editorGutter.addedBackground": "#3C9F4A",
    "editorGutter.deletedBackground": "#A22929",
    "editorGutter.modifiedBackground": "#6f4d81",
    // editorGroup
    "editorGroup.border": "#1D181F",
    "editorGroup.dropBackground": "#1D181F99",
    // editorGroupHeader
    "editorGroupHeader.noTabsBackground": "#161217",
    "editorGroupHeader.tabsBackground": "#161217",
    "editorGroupHeader.tabsBorder": "#1D181F",
    // editorHoverWidget
    "editorHoverWidget.background": "#1D181F",
    "editorHoverWidget.border": "#000",
    "editorIndentGuide.background": "#3B5364",
    "editorLineNumber.foreground": "#aaa",
    "editorLink.activeForeground": "#aaa",
    // editorMarkerNavigation
    "editorMarkerNavigation.background": "#3B536433",
    "editorMarkerNavigationError.background": "#A22929",
    "editorMarkerNavigationWarning.background": "#7EE5D4",
    // ruler
    "editorOverviewRuler.border": "#000",
    "editorOverviewRuler.commonContentForeground": "#7EE5D455",
    "editorOverviewRuler.currentContentForeground": "#ee3a4355",
    "editorOverviewRuler.incomingContentForeground": "#C9BFFF55",
    "editorRuler.foreground": "#AE91BB",
    // editorSuggestWidget
    "editorSuggestWidget.background": "#1D181F",
    "editorSuggestWidget.border": "#1D181F",
    "editorSuggestWidget.foreground": "#aaa",
    "editorSuggestWidget.highlightForeground": "#7EE5D4",
    "editorSuggestWidget.selectedBackground": "#0b0b0b",
    // editorWarning
    "editorWarning.border": "#ffffff00",
    "editorWarning.foreground": "#7EE5D4",
    "editorWhitespace.foreground": "#ffffff1a",
    "editorWidget.background": "#1D181F",
    "editorWidget.border": "#000",
    "errorForeground": "#A22929",
    // extensionButton
    "extensionButton.prominentBackground": "#7EE5D4",
    "extensionButton.prominentForeground": "#fff",
    "extensionButton.prominentHoverBackground": "#AE91BB",
    "focusBorder": "#000",
    "foreground": "#aaa",
    // input
    "input.background": "#1D181F",
    "input.border": "#7EE5D4",
    "input.foreground": "#7EE5D4",
    "input.placeholderForeground": "#aaa",
    "inputOption.activeBorder": "#8dffff",
    "inputValidation.errorBackground": "#0b0b0b",
    "inputValidation.errorBorder": "#7EE5D4",
    "inputValidation.infoBackground": "#0b0b0b",
    "inputValidation.infoBorder": "#000",
    "inputValidation.warningBackground": "#0b0b0b",
    "inputValidation.warningBorder": "#7EE5D4",
    // list
    "list.activeSelectionBackground": "#161217",
    "list.activeSelectionForeground": "#7EE5D4",
    "list.dropBackground": "#161217",
    "list.focusBackground": "#161217",
    "list.focusForeground": "#7EE5D4",
    "list.highlightForeground": "#7EE5D4",
    "list.hoverBackground": "#161217",
    "list.hoverForeground": "#7EE5D4",
    "list.inactiveSelectionBackground": "#161217",
    "list.inactiveSelectionForeground": "#7EE5D4",
    // merge
    "merge.border": "#ffffff00",
    "merge.commonContentBackground": "#ffffff00",
    "merge.commonHeaderBackground": "#ffffff00",
    "merge.currentContentBackground": "#ffffff00",
    "merge.currentHeaderBackground": "#ffffff00",
    "merge.incomingContentBackground": "#ffffff00",
    "merge.incomingHeaderBackground": "#ffffff00",
    // panel
    "panel.background": "#1D181F",
    "panelTitle.activeBorder": "#7EE5D4",
    "panelTitle.activeForeground": "#7EE5D4",
    "panelTitle.inactiveForeground": "#aaa",
    // "peekView
    "peekView.border": "#7EE5D4",
    "peekViewEditor.background": "#0b0b0b",
    "peekViewEditor.matchHighlightBackground": "#0b0b0b00",
    "peekViewEditorGutter.background": "#1D181F",
    "peekViewResult.background": "#1D181F",
    "peekViewResult.fileForeground": "#aaa",
    "peekViewResult.lineForeground": "#fff",
    "peekViewResult.matchHighlightBackground": "#000",
    "peekViewResult.selectionBackground": "#593436",
    "peekViewResult.selectionForeground": "#fff",
    "peekViewTitle.background": "#1D181F",
    "peekViewTitleDescription.foreground": "#aaa",
    "peekViewTitleLabel.foreground": "#7EE5D4",
    // picker
    "pickerGroup.border": "#000",
    "pickerGroup.foreground": "#aaa",
    // progressBar
    "progressBar.background": "#7EE5D4",
    // scrollbar
    "scrollbar.shadow": "#00000000",
    "scrollbarSlider.activeBackground": "#413354",
    "scrollbarSlider.background": "#474051",
    "scrollbarSlider.hoverBackground": "#413354",
    // selection
    "selection.background": "#aaaaaa",
    // sidebar
    "sideBar.background": "#1D181F",
    // "sideBar.border": "#7EE5D4",
    "sideBar.foreground": "#fff",
    "sideBarSectionHeader.background": "#161217",
    "sideBarSectionHeader.foreground": "#aaaaaa",
    "sideBarTitle.foreground": "#aaaaaa",
    // statusBar
    "statusBar.background": "#161217",
    "statusBar.border": "#161217",
    "statusBar.foreground": "#fff",
    "statusBar.debuggingBackground": "#161217",
    "statusBar.debuggingForeground": "#fff",
    "statusBar.noFolderBackground": "#161217",
    "statusBar.noFolderForeground": "#aaa",
    "statusBarItem.activeBackground": "#413354",
    "statusBarItem.hoverBackground": "#474051",
    "statusBarItem.prominentBackground": "#161217",
    "statusBarItem.prominentHoverBackground": "#fff",
    // tab
    "tab.activeBackground": "#161217",
    "tab.activeForeground": "#fff",
    "tab.border": "#1D181F",
    "tab.activeBorder": "#7EE5D4",
    "tab.inactiveBackground": "#1D181F",
    "tab.inactiveForeground": "#aaa",
    "tab.unfocusedActiveForeground": "#aaa",
    "tab.unfocusedInactiveForeground": "#aaa",
    // --- workbench: terminal
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#aaa",
    "terminal.ansiGreen": "#C9BFFF",
    "terminal.ansiYellow": "#7EE5D4",
    "terminal.ansiBlue": "#0088ff",
    "terminal.ansiMagenta": "#C9BFFF",
    "terminal.ansiCyan": "#80fcff",
    "terminal.ansiWhite": "#ffffff",
    "terminal.ansiBrightBlack": "#0050A4",
    "terminal.ansiBrightRed": "#aaa",
    "terminal.ansiBrightGreen": "#C9BFFF",
    "terminal.ansiBrightYellow": "#7EE5D4",
    "terminal.ansiBrightBlue": "#0088ff",
    "terminal.ansiBrightMagenta": "#C9BFFF",
    "terminal.ansiBrightCyan": "#80fcff",
    "terminal.ansiBrightWhite": "#7EE5D4",
    "terminal.background": "#1D181F",
    "terminal.foreground": "#ffffff",
    "terminalCursor.background": "#7EE5D4",
    "terminalCursor.foreground": "#7EE5D4",
    // Git status colors in File Explorer
    "gitDecoration.modifiedResourceForeground": "#7EE5D4",
    "gitDecoration.deletedResourceForeground": "#aaa",
    "gitDecoration.untrackedResourceForeground": "#C9BFFF",
    "gitDecoration.ignoredResourceForeground": "#aaa",
    "gitDecoration.conflictingResourceForeground": "#FF7200",
    // textBlockQuote
    "textBlockQuote.background": "#0b0b0b",
    "textBlockQuote.border": "#AE91BB",
    "textCodeBlock.background": "#0b0b0b",
    "textLink.activeForeground": "#AE91BB",
    "textLink.foreground": "#AE91BB",
    "textPreformat.foreground": "#7EE5D4",
    "textSeparator.foreground": "#000",
    "titleBar.activeBackground": "#1D181F",
    "titleBar.activeForeground": "#ffffff",
    "titleBar.inactiveBackground": "#0b0b0b",
    "titleBar.inactiveForeground": "#ffffff33",
    "walkThrough.embeddedEditorBackground": "#000",
    "welcomePage.buttonBackground": "#0b0b0b",
    "welcomePage.buttonHoverBackground": "#000",
    "widget.shadow": "#00000026"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#C8BBA0"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Entity",
      "scope": "entity",
      "settings": {
        "foreground": "#7EE5D4"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#E59B7E"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword, storage.type.class.js",
      "settings": {
        "foreground": "#AE91BB"
      }
    },
    {
      "name": "Meta",
      "scope": "meta",
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Meta Brace",
      "scope": "meta.brace",
      "settings": {
        "foreground": "#7EE5D4"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Punctuation Parameters",
      "scope": "punctuation.definition.parameters",
      "settings": {
        "foreground": "#F0A3FF"
      }
    },
    {
      "name": "Punctuation Template Expression",
      "scope": "punctuation.definition.template-expression",
      "settings": {
        "foreground": "#F0A3FF"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#7EE5D4"
      }
    },
    {
      "name": "Storage Type Arrow Function",
      "scope": "storage.type.function.arrow",
      "settings": {
        "foreground": "#7EE5D4"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#F0A3FF"
      }
    },
    {
      "name": "String Template",
      "scope": [
        "string.template",
        "punctuation.definition.string.template"
      ],
      "settings": {
        "foreground": "#7EE5D4"
      }
    },
    {
      "name": "Support",
      "scope": "support",
      "settings": {
        "foreground": "#C9BFFF"
      }
    },
    {
      "name": "Support Function",
      "scope": "support.function",
      "settings": {
        "foreground": "#AE91BB"
      }
    },
    {
      "name": "Support Variable Property DOM",
      "scope": "support.variable.property.dom",
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "[CSS] - Entity",
      "scope": [
        "source.css entity",
        "source.stylus entity"
      ],
      "settings": {
        "foreground": "#C9BFFF"
      }
    },
    {
      "name": "[CSS] - ID Selector",
      "scope": "entity.other.attribute-name.id.css",
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "[CSS] - Element Selector",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#C9BFFF"
      }
    },
    {
      "name": "[CSS] - Support",
      "scope": [
        "source.css support",
        "source.stylus support"
      ],
      "settings": {
        "foreground": "#7EE5D4"
      }
    },
    {
      "name": "[CSS] - Constant",
      "scope": [
        "source.css constant",
        "source.css support.constant",
        "source.stylus constant",
        "source.stylus support.constant"
      ],
      "settings": {
        "foreground": "#F0A3FF"
      }
    },
    {
      "name": "[CSS] - String",
      "scope": [
        "source.css string",
        "source.css punctuation.definition.string",
        "source.stylus string",
        "source.stylus punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#F0A3FF"
      }
    },
    {
      "name": "[CSS] - Variable",
      "scope": [
        "source.css variable",
        "source.stylus variable"
      ],
      "settings": {
        "foreground": "#C9BFFF"
      }
    },
    {
      "name": "[HTML] - Entity Name",
      "scope": "text.html.basic entity.name",
      "settings": {
        "foreground": "#C9BFFF"
      }
    },
    {
      "name": "[HTML] - ID value",
      "scope": "meta.toc-list.id.html",
      "settings": {
        "foreground": "#F0A3FF"
      }
    },
    {
      "name": "[HTML] - Entity Other",
      "scope": "text.html.basic entity.other",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7EE5D4"
      }
    },
    {
      "name": "[HTML] - Script Tag",
      "scope": "meta.tag.metadata.script.html entity.name.tag.html",
      "settings": {
        "foreground": "#7EE5D4"
      }
    },
    {
      "name": "[HTML] - Quotes. these are a slightly different colour because expand selection will then not include quotes",
      "scope": "punctuation.definition.string.begin, punctuation.definition.string.end",
      "settings": {
        "foreground": "#aaa"
      }
    },
    {
      "name": "[INI] - Entity",
      "scope": "source.ini entity",
      "settings": {
        "foreground": "#e1efff"
      }
    },
    {
      "name": "[INI] - Keyword",
      "scope": "source.ini keyword",
      "settings": {
        "foreground": "#7EE5D4"
      }
    },
    {
      "name": "[INI] - Punctuation Definition",
      "scope": "source.ini punctuation.definition",
      "settings": {
        "foreground": "#F0A3FF"
      }
    },
    {
      "name": "[INI] - Punctuation Separator",
      "scope": "source.ini punctuation.separator",
      "settings": {
        "foreground": "#AE91BB"
      }
    },
    {
      "name": "[JAVASCRIPT] - Storage Type Function",
      "scope": "source.js storage.type.function",
      "settings": {
        "foreground": "#C9BFFF"
      }
    },
    {
      "name": "[JAVASCRIPT] - Variable Language",
      "scope": "variable.language, entity.name.type.class.js",
      "settings": {
        "foreground": "#C9BFFF"
      }
    },
    {
      "name": "[JAVASCRIPT] - Inherited Component",
      "scope": "entity.other.inherited-class.js",
      "settings": {
        "foreground": "#ccc"
      }
    },
    {
      "name": "[PYTHON] - Variable",
      "scope": [
        "variable.language.special.cls.python",
        "variable.language.special.self.python",
        "variable.legacy.builtin.python",
        "variable.parameter.class.python",
        "variable.parameter.function-call.python",
        "variable.parameter.function.language.python",
        "variable.parameter.function.language.special.cls.python",
				"variable.language.special.self.python",
				"variable.parameter.function.language.special.self.python"
      ],
      "settings": {
        "foreground": "#e1efff"
      }
    },
    {
      "name": "[PYTHON] - Variable",
      "scope": [
        "constant.character.escape.python",
        "constant.character.escape.regexp",
        "constant.character.format.placeholder.other.python",
        "constant.character.set.regexp",
        "constant.character.unicode.regexp",
        "constant.language.python",
        "constant.numeric.bin.python",
        "constant.numeric.dec.python",
        "constant.numeric.float.python",
        "constant.numeric.hex.python",
        "constant.numeric.oct.python",
        "constant.numeric.python",
        "constant.other.caps.python",
        "constant.other.ellipsis.python",
        "constant.other.set.regexp"
      ],
      "settings": {
        "foreground": "#C9BFFF"
      }
    },
    {
      "name": "[PYTHON] - Meta Function Call",
      "scope": [
          "meta.function-call.python",
          "meta.function-call.generic.python",
      ],
      "settings": { "foreground": "#7EE5D4" }
    },
    {
      "name": "[PYTHON] - Punctuation",
      "scope": [
        "punctuation.definition.list.begin.python",
        "punctuation.definition.list.end.python",
        "punctuation.definition.dict.begin.python",
        "punctuation.definition.dict.end.python",
        "punctuation.separator.colon.python",
        "punctuation.section.function.begin.python",
    ],
      "settings": { "foreground": "#fff" },
    },
    {
      "name": "[PYTHON] - Quoted Docstring",
      "scope": "string.quoted.docstring.multi.python",
      "settings": {
        "foreground": "#7EE5D4"
      }
    },
    {
      "name": "[PYTHON] - Self Argument",
      "scope": "variable.parameter.function.language.special.self.python",
      "settings": {
        "foreground": "#C9BFFF"
      }
    },
    {
      "name": "[JSON] - Support",
      "scope": "source.json support",
      "settings": {
        "foreground": "#7EE5D4"
      }
    },
    {
      "name": "[JSON] - String",
      "scope": [
        "source.json string",
        "source.json punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#e1efff"
      }
    },
    {
      "name": "[MARKDOWN] - Heading Punctuation",
      "scope": "punctuation.definition.heading.markdown",
      "settings": {
        "foreground": "#e1efff"
      }
    },
    {
      "name": "[MARKDOWN] - Heading Name Section",
      "scope": [
        "entity.name.section.markdown",
        "markup.heading.setext.1.markdown",
        "markup.heading.setext.2.markdown"
